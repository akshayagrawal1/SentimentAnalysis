    "from textblob import TextBlob\n",
    "\n",
    "line = \"can be improved but good enough\"\n",
    "analysis = TextBlob(line)\n",
    "print(analysis.sentiment)\n",
    "if analysis.sentiment.polarity >= 0.25:\n",
    "    print(\"positive\")\n",
    "elif analysis.sentiment.polarity <= -0.25:\n",
    "    print(\"negative\")\n",
    "else:\n",
    "    print(\"neutral\")"
